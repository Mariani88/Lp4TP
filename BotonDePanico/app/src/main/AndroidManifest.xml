<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lenguajes4.botondepanico" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ProcesoDeActivacionActivity"
            android:label="@string/title_activity_proceso_de_activacion" >
        </activity>
        <activity
            android:name=".CancelarActivity"
            android:label="@string/title_activity_cancelar" >
        </activity>
        <activity
            android:name=".EnvioDeSMSActivity"
            android:label="@string/title_activity_envio_de_sms" >
        </activity>
    </application>

</manifest>
